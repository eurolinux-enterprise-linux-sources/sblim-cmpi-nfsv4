********************************************************************************
class : Linux_NFSv4SystemSetting

--------------------------------------------------------------------------------
Linux_NFSv4SystemSetting : -eq -cmd egrep "^/.*" /etc/exports | wc -l

--------------------------------------------------------------------------------
SystemCreationClassName : -eq Linux_ComputerSystem
SystemName : -eq -cmd hostname -f
CreationClassName : -eq Linux_NFSv4SystemSetting
Filename : -eq /etc/exports
Readable : -eq -cmd if [[ `stat -c "%A" /etc/exports` = ?r* ]]; then echo TRUE; else echo FALSE; fi
Writeable : -eq -cmd if [[ `stat -c "%A" /etc/exports` = ??w* ]]; then echo TRUE; else echo FALSE; fi
SettingID : -set
Directory : -set
